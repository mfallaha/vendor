{
    "name": "asasmoyo/yii2-saml",
    "description": "Connect Yii 2 application to a Saml Identity Provider for Single Sign On",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "saml",
        "single sign on"
    ],
    "license": "MIT",
    "authors": [{
            "name": "Arba Sasmoyo",
            "email": "asasmoyo@student.uns.ac.id"
        },
        {
            "name": "Muhammad Safri Juliardi",
            "email": "ardi93@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "yiisoft/yii2": "^2.0.28",
        "onelogin/php-saml": "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.34",
        "doctrine/instantiator": "1.0.5"
    },
    "autoload": {
        "psr-4": {
            "asasmoyo\\yii2saml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "asasmoyo\\yii2saml\\tests\\": "tests/"
        }
    }
}
